{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.19.4",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "ESNext",
    "target": "ES2020",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strictNullChecks": true
  }
}